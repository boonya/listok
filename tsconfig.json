{
  "compilerOptions": {
    /** Base options */
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    /** Strictness */
    "strict": true,
    "strictNullChecks": true
  },
  "exclude": ["*.test.tsx?"]
}
